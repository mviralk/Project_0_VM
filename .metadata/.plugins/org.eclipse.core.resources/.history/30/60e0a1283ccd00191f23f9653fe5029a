import com.mistbank.exceptions.BusinessException;
import com.mistbank.model.MistUser;

public class MistbankDaoImp implements MistbankDAO {

	@Override
	public MistUser useraccountcreatesavings(MistUser mistUser) throws BusinessException {
		MistUser mistuser = null;
		try(Connection connection = OracleConnection.getConnection()){
			String sql = "call REGISTERSAVINSUSER(?,?,?,?,?,?,?,?,?,?,?,?)";
			CallableStatement callabletatement = connection.prepareCall(sql);
			
		} catch (ClassNotFoundException | SQLException e) {
			System.out.println(e);
			throw new BusinessException("Internal error occured");
		}
		return mistuser;
	}

}
